<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textColor" format="color"/>
    <attr name="pressedTextColor" format="color"/>
    <attr name="text" format="string"/>
    <attr name="textSize" format="dimension"/>
    <attr name="normalIcon" format="reference"/>
    <attr name="pressedIcon" format="reference"/>

    <declare-styleable name="IconTextView">
        <attr name="textColor"/>
        <attr name="pressedTextColor"/>
        <attr name="text"/>
        <attr name="textSize"/>
        <attr name="normalIcon"/>
        <attr name="pressedIcon"/>
    </declare-styleable>
</resources>